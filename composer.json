{
  "name": "cmk/posts-by-tabs",
  "description": "React Wordpress Theme",
  "license": "GPL-3.0-or-later",
  "version": "1.0.4",
  "authors": [
      {
        "name": "CÃ©dric Moris Kelly",
          "email": "cedric@moriskelly.com"
      }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=8.1.2"
  },
  "autoload": {
    "psr-4": {
      "Cmk\\PostsByTabs\\": "inc/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^3.0",
    "squizlabs/php_codesniffer": "^3.11"
  },
  "scripts": {
    "build": [
      "@composer --no-plugins --no-interaction install --no-scripts --no-dev",
      "@composer --no-plugins --no-interaction archive"
    ],
    "lint": "phpcs --standard=Wordpress --extensions=php inc/",
    "fix": "phpcbf --standard=Wordpress --extensions=php inc/",
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  },
  "scripts-descriptions": {
    "build": "Build project artifact."
  }
  
}
